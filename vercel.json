{
    "version": 2,
    "builds": [
      { "src": "api/sendMessage.js", "use": "@vercel/node" },
      { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/sendMessage", "dest": "/api/sendMessage.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }